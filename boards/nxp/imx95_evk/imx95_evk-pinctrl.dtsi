#include <nxp/nxp_imx/mimx9596avzxn-pinctrl.dtsi>

&pinctrl {

	uart1_default: uart1_default {
		group0 {
			pinmux = <&iomuxc_uart1_rxd__lpuart1_rx>,
				<&iomuxc_uart1_txd__lpuart1_tx>;
			bias-pull-up;
			slew-rate = "slightly_fast";
			drive-strength = "x5";
		};
	};

	lpuart3_default: lpuart3_default {
		group0 {
			pinmux = <&iomuxc_gpio_io15__lpuart3_rx>,
				<&iomuxc_gpio_io14__lpuart3_tx>;
			bias-pull-up;
			slew-rate = "slow";
			drive-strength = "x1";
		};
	};
};
